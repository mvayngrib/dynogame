  "dependencies": {
    "@tradle/build-resource": "^2.9.1",
    "@tradle/custom-models": "github:tradle/custom-models",
    "@tradle/dynamodb": "github:tradle/dynamodb",
    "@tradle/engine": "^4.10.0",
    "@tradle/gen-samples": "github:tradle/gen-samples",
    "@tradle/merge-models": "^2.1.0",
    "@tradle/models": "github:tradle/models",
    "@tradle/schema-graphql": "github:tradle/schema-graphql",
    "@tradle/schema-joi": "tradle/schema-joi",
    "@tradle/validate-model": "^2.2.0",
    "@tradle/validate-resource": "^2.8.0",
    "apollo-client": "^1.9.0-0",
    "aws-sdk": "^2.100.0",
    "co": "^4.6.0",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "dynogels": "mvayngrib/dynogels",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "graphql-tag": "^2.4.2",
    "isomorphic-fetch": "^2.2.1",
    "joi": "10.5.0",
    "lowdb": "^0.16.2"
  },
  "scripts": {
    "db": "cd dynamo-local && docker-compose up",
    "api": "yarn start",
    "dynamo": "cd dynamo-local && docker-compose down && docker-compose up",
    "gensamples": "tradle-samples ./samples-conf.js",
    "loadsamples": "yarn loadfixtures",
    "loadfixtures": "DEBUG=*tradle* node ./example/load-fixtures.js ./example/fixtures.json",
    "start": "yarn server",
    "server": "DEBUG=*tradle* node example/server",
    "client": "DEBUG=*tradle* node example/client",
    "debug": "yarn debug:server",
    "debug-brk": "yarn debug-brk:server",
    "debug:server": "DEBUG=dynogels,*tradle* node --debug --inspect example/server",
    "debug-brk:server": "DEBUG=dynogels,*tradle* node --debug-brk --inspect example/server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tradle/dynamodb-graphql-example"
  }
